<button class="btn btn-default" ng-click="addjoiner()">AddJoiner</button>
<hr>
<form ng-submit="postOrder()">
  <div id='joiner' class='container' ng-repeat="item in items" style="margin-bottom:20px">
  	<div class="row">
		<div id="name" class="col-md-3">
		    <div class="input-group">
		      <div class="input-group-btn">
		        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">已有人员 <span class="caret"></span></button>
		        <ul class="dropdown-menu">
		        	<li ng-repeat="user in usersInfo"><a href="#" ng-click="setvalueforinput($parent.$index, user.name)">{{user.name}}</a></li>
		        </ul>
		      </div><!-- /btn-group -->
		      <input type="text" class="form-control" ng-model="item.name">
		  </div>	
		</div>
		<div id="pwd" class="col-md-3">
			<input type="text" class="form-control" placeholder="money" class="form-control" ng-model="item.money">
		</div>
		<div class="col-md-1">
			<a href="#"><span class="glyphicon glyphicon-minus" ng-click="removejoiner($index)"></span></a>
		</div>
  	</div>
  </div>
  <button type="submit" class="btn btn-primary">Start</button>
</form>